{
    "name": "super-admin",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "lint-staged": {
        "src/**/*.{ts,tsx,scss}": [
            "prettier --write"
        ],
        "./*.{js,cjs,html,jsx,josn,md,ts,tsx}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "dev": "vite  --host ",
        "format": "prettier --write \"src/**/*.{ts,tsx}\" \"./*.{js,cjs,html,jsx,json,md,ts,tsx}\"",
        "build": "tsc && vite build",
        "build-analyze": "cross-env ANALYZE=true vite build",
        "build-gh": "cross-env BASE_SITE_URL=/super-admin/ vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ant-design/cssinjs": "^1.18.4",
        "@ant-design/icons": "^5.3.5",
        "@ant-design/pro-components": "^2.6.51",
        "@dnd-kit/core": "^6.1.0",
        "@dnd-kit/sortable": "^8.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@emotion/react": "^11.11.4",
        "@reduxjs/toolkit": "^1.9.7",
        "@wangeditor/editor": "^5.1.23",
        "@wangeditor/editor-for-react": "^1.0.6",
        "antd": "^5.15.3",
        "axios": "^1.6.8",
        "color": "^4.2.3",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.10",
        "fortea": "^1.0.8",
        "i18next-browser-languagedetector": "^7.2.0",
        "keepalive-for-react": "^1.0.22",
        "nookies": "^2.5.2",
        "nprogress": "^0.2.0",
        "qs": "^6.12.0",
        "ramda": "^0.29.1",
        "react": "^18.2.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.22.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@emotion/babel-preset-css-prop": "^11.11.0",
        "@types/node": "^20.11.30",
        "@types/nprogress": "^0.2.3",
        "@types/ramda": "^0.29.11",
        "@types/react": "^18.2.67",
        "@types/react-dom": "^18.2.22",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.2",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "rollup-plugin-visualizer": "^5.12.0",
        "sass": "^1.72.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.3",
        "vite": "^5.2.2",
        "vite-plugin-compression": "^0.5.1"
    }
}
