{
    "name": "super-admin",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "lint-staged": {
        "src/**/*.{ts,tsx,scss}": [
            "prettier --write"
        ],
        "./*.{js,cjs,html,jsx,json,md,ts,tsx}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "update": "git add . && git commit -m \"update\" && git push",
        "build-gh": "cross-env VITE_BASE_SITE_URL=/super-admin/ vite build",
        "format": "prettier --write \"src/**/*.{ts,tsx}\" \"./*.{js,cjs,html,jsx,json,md,ts,tsx}\"",
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prepare": "husky",
        "msw-init": "npx msw init public/ --save"
    },
    "dependencies": {
        "@ant-design/cssinjs": "^1.21.1",
        "@ant-design/icons": "^5.4.0",
        "@emotion/react": "^11.13.3",
        "@tabler/icons-react": "^3.12.0",
        "ahooks": "^3.8.1",
        "antd": "^5.20.3",
        "axios": "^1.7.5",
        "color": "^4.2.3",
        "dayjs": "^1.11.13",
        "fortea": "^1.0.9",
        "i18next": "^23.14.0",
        "i18next-browser-languagedetector": "^7.2.1",
        "keepalive-for-react": "^2.0.11",
        "lodash": "^4.17.21",
        "react": "18.2.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "18.2.0",
        "react-i18next": "^14.1.3",
        "react-router-dom": "^6.26.1",
        "screenfull": "^6.0.2",
        "simplebar-react": "^3.2.6",
        "uuid": "^10.0.0",
        "zustand": "^4.5.5"
    },
    "devDependencies": {
        "@emotion/babel-preset-css-prop": "^11.12.0",
        "@faker-js/faker": "^8.4.1",
        "@types/color": "^3.0.6",
        "@types/lodash": "^4.17.7",
        "@types/node": "^20.16.1",
        "@types/react": "^18.3.4",
        "@types/react-beautiful-dnd": "^13.1.8",
        "@types/react-dom": "18.2.22",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.11",
        "husky": "^9.1.5",
        "lint-staged": "^15.2.9",
        "msw": "^2.3.5",
        "postcss": "^8.4.41",
        "prettier": "^3.3.3",
        "sass": "^1.77.8",
        "tailwindcss": "^3.4.10",
        "typescript": "^5.5.4",
        "vite": "^5.4.2"
    }
}
